{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create": "npx sequelize db:create",
    "db:migrate": "npx sequelize db:migrate",
    "db:undoMigrate": "npx sequelize db:migrate:undo:all",
    "db:seedAll": "npx sequelize db:seed:all",
    "db:drop": "npx sequelize db:drop",
    "db:remigrate": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all",
    "db:user": "npx sequelize-cli model:generate --name User --attributes username:string,email:string,password:string",
    "db:exercise": "npx sequelize-cli model:generate --name Exercise --attributes name:string,description:string,muscleGroup:string,type:string,equipment:string,points:integer,calories:string",
    "db:plan": "npx sequelize-cli model:generate --name Plan --attributes name:string,image:string,description:string,equipment:string,points:integer,calories:string",
    "db:planExercise": "npx sequelize-cli model:generate --name PlanExercise --attributes planId:integer,exerciseId:integer",
    "db:session": "npx sequelize-cli model:generate --name Session --attributes planId:integer,userId:integer",
    "db:day": "npx sequelize-cli model:generate --name Day --attributes planId:integer,points:integer",
    "db:dayExercise": "npx sequelize-cli model:generate --name DayExercise --attributes dayId:integer,exerciseId:integer",
    "db:userDays": "npx sequelize-cli model:generate --name UserDay --attributes userId:integer,dayId:integer,isCompleted:boolean",
    "db:seed:generateUser": "npx sequelize-cli seed:generate --name User",
    "db:seed:generateExercise": "npx sequelize-cli seed:generate --name Exercise",
    "db:seed:generateDayExercise": "npx sequelize-cli seed:generate --name DayExercise",
    "db:seed:generatePlan": "npx sequelize-cli seed:generate --name Plan",
    "db:seed:generateDay": "npx sequelize-cli seed:generate --name Days",
    "dev": "nodemon --env-file=.env ./src/app.js --ext js,jsx"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.9",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "xlsx": "^0.18.5"
  }
}
